    <div class="page-wrapper bg-gra-02 p-t-130 p-b-100 font-poppins">
        <div class="wrapper wrapper--w680">
            <div class="card card-4">
                <div class="card-body">
                    <h2 class="title">Soumisson de prêt</h2>
                    <form method="post" action="/register">
                        <div class="row row-space">
                            <div class="col-2">
                                <div class="input-group">
                                    <label class="label">Pseudo</label>
                                    <input class="input--style-4" type="text" name="pseudo" required autofocus>
                                </div>
                            </div>
                            <div class="col-2">
                                <div class="input-group">
                                    <label class="label">Montant</label>
                                    <input class="input--style-4" type="text" onkeypress="return onlyNumberKey(event)" name="montant" required autofocus>
                                </div>
                                <script>
                                    function onlyNumberKey(evt) {
                                          
                                        // Only ASCII charactar in that range allowed
                                        var ASCIICode = (evt.which) ? evt.which : evt.keyCode
                                        if (ASCIICode > 31 && (ASCIICode < 48 || ASCIICode > 57))
                                            return false;
                                        return true;
                                    }
                                </script>
                            </div>
                        </div>
                        <div class="row row-space">
                            <div class="col-2">
                                <div class="input-group">
                                    <label class="label">Quartier</label>
                                    <div class="input-group">
                                        <input class="input--style-4 js-datepicker" type="quartier" name="quartier" required autofocus>
                                    </div>
                                </div>
                            </div>
                            <div class="col-2">
                                <div class="input-group">
                                    <label class="label">Delai de remboursement</label>
                                    <div class="p-t-10">
                                        <label class="radio-container m-r-45">7jours -- taux 10%
                                            <input type="radio" checked="checked" name="delai" required autofocus>
                                            <span class="checkmark"></span>
                                        </label>
                                        <label class="radio-container"> 14jours -- taux 14%
                                            <input type="radio" name="delai" required autofocus>
                                            <span class="checkmark"></span>
                                        </label>
                                    </div>
                                </div>
                            </div>
                        </div>
                        <div class="row row-space">
                            <div class="col-2">
                                <div class="input-group">
                                    <label for="motifs">Motifs de prêt:</label>

                                        <select name="motifs" id="motifs">
                                            <option value="">--Séléctionner un choix--</option>
                                            <option value="dog">Nourriture</option>
                                            <option value="cat">Médicaments</option>
                                            <option value="hamster">Facture d'eau</option>
                                            <option value="parrot">Facture d'éléctricité</option>
                                            <option value="spider">Transport</option>
                                            <option value="goldfish">Autres</option>
                                        </select>
                                </div>
                            </div>
                            <div class="col-2">
                                <div class="input-group">
                                    <label class="label">Numéro de téléphone</label>
                                    <input class="input--style-4" type="text" name="phone" placeholder="Numéro Flooz(Moov)" required autofocus>
                                </div>
                            </div>
                        </div>
                           <div class="input-group">
                                    <div class="p-t-10">
                                        <label class="label">Voulez-vous que vos informations soit visible?</label>
                                        <label class="radio-container m-r-45">Oui
